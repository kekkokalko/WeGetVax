                  <Font size="17.0" />
               </font>
            </Label>
            <TableView fx:id="tabella" layoutX="51.0" layoutY="366.0" prefHeight="301.0" prefWidth="890.0">
               <columns>
                  <TableColumn fx:id="data" prefWidth="103.0" text="Data" />
                  <TableColumn fx:id="luogo" prefWidth="162.0" text="Luogo" />
                  <TableColumn fx:id="citta" prefWidth="157.0" text="Città" />
                  <TableColumn fx:id="via" prefWidth="145.0" text="Via" />
                  <TableColumn fx:id="civico" prefWidth="75.0" text="Civico" />
                  <TableColumn fx:id="provincia" prefWidth="75.0" text="Provincia" />
                  <TableColumn fx:id="asl" prefWidth="97.0" text="Asl" />
                  <TableColumn fx:id="distretto" prefWidth="76.0" text="Distretto" />
               </columns>
            </TableView>
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="580.0" layoutY="146.0" pickOnBounds="true" preserveRatio="true">
