<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.wegetvax.Controllers.PrenotazioneController">
   <children>
      <AnchorPane layoutX="4.0" layoutY="1.0" prefHeight="800.0" prefWidth="1000.0">
         <children>
            <Button fx:id="indietro" layoutX="710.0" layoutY="711.0" mnemonicParsing="false" onAction="#back" prefHeight="44.0" prefWidth="177.0" style="-fx-background-radius: 30; -fx-background-color: GREY;" text="Indietro" textFill="WHITE">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <VBox layoutX="-8.0" layoutY="-6.0" prefHeight="124.0" prefWidth="1011.0" style="-fx-background-color: #357EC7;" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="196.0" layoutY="146.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../Icone/Logo.png" />
               </image>
            </ImageView>
            <Label fx:id="user" layoutX="765.0" layoutY="137.0" prefHeight="18.0" prefWidth="194.0">
               <font>
                  <Font name="Calibri Bold" size="15.0" />
               </font>
            </Label>
            <Label layoutX="64.0" layoutY="333.0" prefHeight="26.0" prefWidth="456.0" text="Le tue prenotazioni:" textFill="#357ec7">
