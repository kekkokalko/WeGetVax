                  <Image url="@../../../Icone/prenotazione.png" />
               </image>
            </ImageView>
            <Button fx:id="aggiungi" layoutX="120.0" layoutY="711.0" mnemonicParsing="false" onAction="#newReservation" prefHeight="44.0" prefWidth="345.0" style="-fx-background-radius: 30; -fx-background-color: #357EC7;" text="Aggiungi una nuona prenotazione" textFill="WHITE">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="logOut" layoutX="804.0" layoutY="170.0" mnemonicParsing="false" onAction="#log_out" prefHeight="26.0" prefWidth="116.0" style="-fx-background-radius: 30; -fx-background-color: #FF0000;" text="LogOut" textFill="WHITE">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Label layoutX="436.0" layoutY="776.0" prefHeight="17.0" prefWidth="168.0" text="© 2022 - Francesco Calcopietro" />
